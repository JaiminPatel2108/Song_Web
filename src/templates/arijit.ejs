<%- include("../partials/header.ejs") %>

    <body>
        <div class="main">

            <!-- navbar -->
            <div class="part1">
                <div class="mainlogo">
                    <img src="/images/spotifylogo.png" class="logoimg" alt="" srcset="">
                    <p>Spotify</p>
                </div>


                <div class="cssnavbar">
                    <div class="afterlogo3">
                        <ul>
                            <li><a href="/" class="link"><i class="fa-solid fa-house"></i> Home</a></li>
                            <li><i class="fa-solid fa-magnifying-glass"></i> Search</li>
                            <li><i class="fa-solid fa-bookmark"></i> Your Librery</li>
                        </ul>
                    </div>
                    <div class="afterlogo2">
                        <ul>
                            <li><i class="fa-solid fa-headphones"></i> Created Playlist</li>
                            <li><i class="fa-solid fa-heart"></i> Liked Songs</li>
                        </ul>
                    </div>
                    <div class="afterlogolast3">
                        <div><input type="text" class="searchnav" id="searchnav" placeholder="Search Singer.."
                                onkeyup="searchsinger()"></div>
                        <ul>
                            <li><a href="/arijit" class="link singername">Arijit Singh</a></li>
                            <li><a href="/pritam" class="link singername">Pritam</a></li>
                            <li><a href="/yoyo" class="link singername">YoYo HoneySingh</a></li>
                            <li><a href="/kishore" class="link singername">Kishore Kumar</a></li>
                            <!-- <li><i class="fa-solid fa-bars"></i></li> -->
                        </ul>
                    </div>
                </div>

            </div>
            

            <div class="part2">
                <div class="upper">
                    <div class="name">Arijit Signh</div>
                    <div class="count">40,512,796 monthly listener &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp
                        &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp
                        &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp
                        &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp
                        &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp;
                        <%= user.email%>
                    </div>
                </div>
                <div class="lower">
                    <div class="populer">
                        <div>populer</div>
                        <div><input type="text" class="search" id="search" placeholder="Search Any Song Here You Like.."
                                onkeyup="onkeyup"></div>
                    </div>
                    <div class="songlist" id="songlist"></div>
                </div>
            </div>
        </div>

        <div class="bottom">
            <div class="bottom1">
                <div class="bottomsongname" id="songname">
                    Song Name
                </div>
                <div class="bottomicon">
                    <i class="fa-regular fa-heart" id="heart"></i>
                </div>
            </div>
            <div class="bottom2">
                <div>
                    <ul class="icons">
                        <li><i class="fa-solid fa-backward-step" id="backbtn"></i></li>
                        <li><i class="fa-solid fa-play" id="playbtn"></i></li>
                        <li><i class="fa-solid fa-forward-step" id="nextbtn"></i></li>
                    </ul>
                </div>
                <div class="icon">
                    <li class="startingtime" id="songcurrenttimeupdate">0</li>
                    <input type="range" class="range" value="0" min="0" max="100" id="range">
                    <li class="endingtime" id="songfulltime">100</li>
                </div>
            </div>
            <div class="bottom3">
                <div class="bottom3icon">
                    <i class="fa-solid fa-volume-high" id="volume"></i>
                </div>
                <div class="bottom3range">
                    <input class="bottom3range" id="volumerange" type="range" value="50" min="0" max="100">
                </div>
            </div>
        </div>
        <script src="/js/arijit.js"></script>
    </body>

    </html>