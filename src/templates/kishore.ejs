<%- include("../partials/header.ejs") %>
<body>
    <div class="main">
        <%- include("../partials/navbar.ejs") %>
    <div class="part2">
            <div class="upper">
                <div class="name">Kishore Kumar</div>
                <div class="count">28,298,794 monthly listener &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp; 
                    <%= user.email%></div>
            </div>
            <div class="lower">
                <div class="populer">
                    <div>populer</div>
                    <div><input type="text" class="search" id="search" placeholder="Search Any Song Here You Like.." onkeyup="onkeyup"></div>
                </div>
                <div class="songlist" id="songlist"></div>
            </div>
        </div>

    </div>

    <div class="bottom">
        <div class="bottom1">
            <div class="bottomsongname" id="songname">
                Song Name
            </div>
            <div class="bottomicon">
                <i class="fa-regular fa-heart" id="heart"></i>
            </div>
        </div>
        <div class="bottom2">
            <div>
                <ul class="icons">
                    <li><i class="fa-solid fa-backward-step" id="backbtn"></i></li>
                    <li><i class="fa-solid fa-play" id="playbtn"></i></li>
                    <li><i class="fa-solid fa-forward-step" id="nextbtn"></i></li>
                </ul>
            </div>
            <div class="icon">
                <li class="startingtime" id="songcurrenttimeupdate">0</li>
                <input type="range" class="range" value="0" min="0" max="100" id="range">
                <li class="endingtime" id="songfulltime">100</li>
            </div>
        </div>
        <div class="bottom3">
            <div class="bottom3icon">
                 <i class="fa-solid fa-volume-high" id="volume"></i>
            </div>
            <div class="bottom3range">
                <input class="bottom3range" type="range" id="volumerange" value="50" min="0" max="100">
            </div>
        </div>
    </div>
    <script src="/js/kishore.js"></script>
</body>
</html>

